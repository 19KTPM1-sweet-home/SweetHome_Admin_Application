<div class="content-body" style="min-height: 932px;">
    <div class="container-fluid">
        <div class="row page-titles mx-0">
            <div class="col-sm-6 p-md-0">
                <div class="welcome-text">
                    <h4>Hi, Welcome To Account Dashboard! </h4>
                    <span class="ml-1">Admin Dashboard</span>
                </div>
            </div>
        </div>
        <!-- row -->
        <div class="row">
            <div class="col-12">
                <div class="card">
                    <div class="card-body">
                        <div id="admin-account-table" class="table-responsive">
                            <section class="ftco-section">
                                <div class="container ">
                                    <div class="row justify-content-center">
                                        <div class="col-md-6 text-center">
                                            <h2 class="heading-section">User Accounts</h2>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-12">
                                            <div class="table-wrap">
                                                <table class="table table-responsive-xl">
                                                    <thead>
                                                        <tr>
                                                            <th>Full Name</th>
                                                            <th>Username</th>
                                                            <th>Phone Number</th>
                                                            <th>&nbsp;</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody class="table-body">

                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>

                                        
                                    </div>
                                </div>
                            </section>
                        </div>

                        <div class="paging-wrapper">
                            {{!-- JQuery will generate pagination UI here --}}
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--**********************************
            Content body end
        ***********************************-->
    <!--Lock Modal HTML -->
    <div id="lockModal" class="modal fade">
        <div class="modal-dialog modal-confirm">
            <div class="modal-content">
                <div class="modal-header flex-column">
                    <div class="delete-icon-box">
                        <i class="material-icons">&#xE5CD;</i>
                    </div>						
                    <h4 class="modal-title w-100">Are you sure?</h4>	
                    <button type="button" class="close exit-lock-modal-btn" data-dismiss="modal" aria-hidden="true">&times;</button>
                </div>
                <div class="modal-body">
                    <p>Do you really want to lock this user?</p>
                </div>
                <div class="modal-footer justify-content-center">
                    <button type="button" class="btn btn-secondary lock-modal-cancel-btn">Cancel</button>
                    <button type="button" class="btn btn-danger lock-modal-lock-btn">Lock</button>
                </div>
            </div>
        </div>
    </div>
    
</div>

