<!--**********************************
            Content body start
        ***********************************-->
<div class="content-body" style="min-height: 927px;">
    <div class="container-fluid">
        <div class="row page-titles mx-0">
            <div class="col-sm-6 p-md-0">
                <div class="welcome-text">
                    <h4>Hi, welcome back!</h4>
                    <p class="mb-0">Your profile here</p>
                </div>
            </div>
            <div class="col-sm-6 p-md-0 justify-content-sm-end mt-2 mt-sm-0 d-flex">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a href="javascript:void(0)">App</a></li>
                    <li class="breadcrumb-item active"><a href="javascript:void(0)">Profile</a></li>
                </ol>
            </div>
        </div>
        <!-- row -->
        <div class="row">
            <div class="col-lg-12">
                <div class="profile">
                    <div class="profile-head">
                        <div class="photo-content">
                            <div class="cover-photo"></div>
                        </div>
                        <div class="profile-photo">
                            <img src="{{#if admin.avatar}}{{admin.avatar}}{{else}}https://cdn.pixabay.com/photo/2016/08/08/09/17/avatar-1577909_960_720.png{{/if}}" class="img-fluid rounded-circle" style="
                            width: 200px;
                            height: 200px;"
                                alt="avatar">
                        </div>
                        <div class="profile-info">
                            <div class="col">
                                <div class="row">
                                    <div class="col-lg-4 col-sm-4 border-right-1 prf-col">
                                        <div class="profile-name">
                                            <h4 class="text-primary">{{admin.fullName}}</h4>
                                            
                                        </div>
                                    </div>
                                    <div class="col-lg-4 col-sm-4 border-right-1 prf-col">
                                        <div class="profile-email">
                                            {{#if admin.email}}
                                            <h4 class="text-muted">{{admin.email}}</h4>
                                            {{else}}
                                            <h4 class="text-muted">Unknown</h4>
                                            {{/if}}
                                            <p>Email</p>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-lg-4 col-sm-4 prf-col">
                                        <div class="profile-call">
                                            {{#if admin.phoneNumber}}
                                            <h4 class="text-muted">{{admin.phoneNumber}}</h4>
                                            {{else}}
                                            <h4 class="text-muted">Unknown</h4>
                                            {{/if}}
                                            <p>Phone No.</p>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-12">
                <div class="card">
                    <div class="card-body">
                        <div class="profile-tab">
                            <div class="custom-tab-1">
                                <ul class="nav nav-tabs">
                                    <li class="nav-item "><a href="#about-me" data-toggle="tab" class="nav-link active"
                                            aria-selected="true">About
                                            Me</a>
                                    </li>
                                </ul>
                                <div class="tab-content">
                                    <div id="about-me" class="tab-pane fade show active">
                                        <div class="profile-personal-info pt-4 border-bottom-1 pb-4">
                                            <h4 class="text-primary mb-4">Personal Information</h4>
                                            <div class="row mb-4">
                                                <div class="col-3">
                                                    <h5 class="f-w-500">Name <span class="pull-right">:</span>
                                                    </h5>
                                                </div>
                                                <div class="col-9">
                                                    <h6>{{admin.fullName}}</h6>
                                                </div>
                                            </div>
                                            <div class="row mb-4">
                                                <div class="col-3">
                                                    <h5 class="f-w-500">Email <span class="pull-right">:</span>
                                                    </h5>
                                                </div>
                                                {{#if admin.email}}
                                                <div class="col-9">
                                                    <h6>{{admin.email}}</h6>
                                                </div>
                                                {{else}}
                                                <div class="col-9">
                                                    <h6>Unknown</h6>
                                                </div>
                                                {{/if}}
                                            </div>

                                            <div class="row mb-4">
                                                <div class="col-3">
                                                    <h5 class="f-w-500">Address <span class="pull-right">:</span></h5>
                                                </div>
                                                {{#if admin.address}}
                                                <div class="col-9">
                                                    <h6>{{admin.address}}</h6>
                                                </div>
                                                {{else}}
                                                <div class="col-9">
                                                    <h6>Unknown</h6>
                                                </div>
                                                {{/if}}

                                            </div>

                                            <div class="row mb-4">
                                                <div class="col-3">
                                                    <h5 class="f-w-500">Phone Number <span class="pull-right">:</span></h5>
                                                </div>
                                                {{#if admin.phoneNumber}}
                                                <div class="col-9">
                                                    <h6>{{admin.phoneNumber}}</h6>
                                                </div>
                                                {{else}}
                                                <div class="col-9">
                                                    <h6>Unknown</h6>
                                                </div>
                                                {{/if}}

                                            </div>

                                            <div class="row mb-4">
                                                <div class="col-3">
                                                    <h5 class="f-w-500">Status <span class="pull-right">:</span></h5>
                                                </div>
                                                {{#if admin.lock}}
                                                <div class="col-9">
                                                    <h6>{{admin.lock}}</h6>
                                                </div>
                                                {{else}}
                                                <div class="col-9">
                                                    <h6>Unknown</h6>
                                                </div>
                                                {{/if}}

                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<!-- Boostrap JS -->
<script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.10.2/dist/umd/popper.min.js" integrity="sha384-7+zCNj/IqJ95wo16oMtfsKbZ9ccEh31eOz1HGyDuCQ6wgnyJNSYdrPa03rtR1zdB" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.min.js" integrity="sha384-QJHtvGhmr9XOIpI6YVutG+2QOK9T+ZnN4kzFN1RtK3zEFEIsxhlmWl5/YESvpZ13" crossorigin="anonymous"></script>

<!-- Required vendors -->
<script src="/public/javascripts/vendor/global/global.min.js"></script>
<script src="/public/javascripts/js/quixnav-init.js"></script>
<script src="/public/javascripts/js/custom.min.js"></script>
